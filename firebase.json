{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/",
        "function": "graphGateway"
      },
      {
        "source": "/graphql",
        "function": "graphGateway"
      },
      {
        "source": "/view",
        "function": "graphGateway"
      },
      {
        "source": "/.internal/**",
        "function": "graphGateway"
      },
      {
        "source": "/profile",
        "function": "fileUploadGateway"
      },
      {
        "source": "/check",
        "function": "fileUploadGateway"
      }
    ],
    "ignore": ["firebase.json", "**/node_modules/**"]
  }
}
