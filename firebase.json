{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/profile",
        "function": "fileUploadGateway"
      },
      {
        "source": "/check",
        "function": "fileUploadGateway"
      },
      {
        "source": "/og-image/**",
        "function": "og-image"
      }
    ],
    "ignore": ["firebase.json", "**/node_modules/**"]
  }
}
