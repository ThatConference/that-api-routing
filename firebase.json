{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/",
        "run": {
          "serviceId": "gateway",
          "region": "us-central1"
        }
      },
      {
        "source": "/graphql",
        "run": {
          "serviceId": "gateway",
          "region": "us-central1"
        }
      },
      {
        "source": "/view",
        "run": {
          "serviceId": "gateway",
          "region": "us-central1"
        }
      },
      {
        "source": "/.internal/**",
        "run": {
          "serviceId": "gateway",
          "region": "us-central1"
        }
      },
      {
        "source": "/profile",
        "function": "fileUploadGateway"
      },
      {
        "source": "/check",
        "function": "fileUploadGateway"
      },
      {
        "source": "og-image.that.tech/",
        "functions": "og-image"
      }
    ],
    "ignore": ["firebase.json", "**/node_modules/**"]
  }
}
