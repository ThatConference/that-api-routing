{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/",
        "function": "graphGateway"
      },
      {
        "source": "/graphql",
        "function": "graphGateway"
      },
      {
        "source": "/view",
        "function": "graphGateway"
      },
      {
        "source": "/.internal",
        "function": "graphGateway"
      }
    ],
    "ignore": ["firebase.json", "**/node_modules/**"]
  }
}
